	abstract = {In their generally worthwhile discussion of developments in the English language (“Quantitative analysis of culture using millions of digitized books,” Research Article, 14 January, p. 176), J.-B. Michel et al. fall into two common traps. First, they assume that the total number of words published in English is a meaningful statistic. Although this figure seems impressive—and has traditionally been used to validate English speakers' belief that their language is exceptional [e.g., (1)]—it primarily reflects the large number of English speakers and their relatively high per capita publication rates. The statistic has little to do with the number of words available to an individual English speaker. Second, the authors claim that the English lexicon “is enjoying a period of enormous growth” and that “the size of the language” has grown “by over 70\% during the past 50 years.” However, they are not measuring the English lexicon directly; they are measuring the written record of the English lexicon. These two concepts should be kept distinct. If the fossil record shows more dinosaur footprints in one period than another, it does not necessarily mean that there were more dinosaurs—it may be that there was more mud.},
	language = {EN},
	urldate = {2022-01-03},
	journal = {Science},
	author = {Morse-Gagné, Elise E.},
	month = apr,
	year = {2011},
	note = {Publisher: American Association for the Advancement of Science},
	file = {Snapshot:/Users/cbarrie6/Zotero/storage/8NG9C2PG/science.332.6025.html:text/html},
}

@article{schwartz_culturomics_2011,
